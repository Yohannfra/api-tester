{
  "host": "https://jsonplaceholder.typicode.com",
  "doc": "https://jsonplaceholder.typicode.com/guide/",
  "headers": {
    "Authorization": ""
  },
  "paths": {
    "/posts": {
      "Create a post": {
        "method": "POST",
        "headers": { "charset": "UTF-8", "Content-Type": "application/json" },
        "body": { "title": "foo", "body": "bar", "userId": 1 },
        "response": {
          "code": 201,
          "content-json-exact": {
            "id": 101,
            "title": "foo",
            "body": "bar",
            "userId": 1
          }
        }
      },
      "Get posts from user": {
        "method": "GET",
        "queries": { "userId": 1 },
        "response": {
          "code": 200,
          "nb_json_items": 10
        }
      },
      "Delete post": {
        "method": "DELETE",
        "endpoint": "/1",
        "response": {
          "code": 200,
          "content-json-exact": {}
        }
      }
    },
    "/todos": {
      "get all todos": {
        "method": "GET",
        "queries": { "limit": 200 },
        "response": {
          "code": 200,
          "nb_json_items": 200
        }
      }
    },
    "/": {
      "test nothing": {
        "method": "GET"
      },
      "Skipped test": {
        "method": "GET",
        "skip": true
      }
    }
  }
}
